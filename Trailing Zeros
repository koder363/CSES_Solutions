import java.util.*;
import java.util.stream.Collectors;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int count = 0;
        int divide = 5;
        while(divide <= n){
            count += (int)n/divide;
            divide *= 5;
        }
        System.out.println(count);
    }
}
