import java.util.*;
import java.io.*;
 
 
class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int num = 0;
		for(int i = 0; i < n-1; i++){
			num ^= sc.nextInt();
		}
		int xor = 0;
		for(int i = 1; i <= n; i++){
			xor ^= i;
		}
		System.out.println(xor^num);
	}
}
